<h1>Listing blockeds</h1>

<table>
  <thead>
    <tr>
      <th>Ip</th>
      <th>Domain</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blockeds.each do |blocked| %>
      <tr>
        <td><%= blocked.ip %></td>
        <td><%= blocked.domain %></td>
        <td><%= blocked.email %></td>
        <td><%= link_to 'Show', blocked %></td>
        <td><%= link_to 'Edit', edit_blocked_path(blocked) %></td>
        <td><%= link_to 'Destroy', blocked, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blocked', new_blocked_path %>
