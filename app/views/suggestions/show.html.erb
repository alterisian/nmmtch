<%- model_class = Suggestion -%>
<div class="page-header">
  <h1><%=t '.title', :default => Suggestion %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:user_1) %></dt>
      <dd><pre class="prettyprint"><%= @suggestion.user_1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:user_2) %></dt>
      <dd><pre class="prettyprint"><%= @suggestion.user_2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:accepted_by_user_1) %></dt>
      <dd><pre class="prettyprint"><%= @suggestion.accepted_by_user_1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:accepted_by_user_2) %></dt>
      <dd><pre class="prettyprint"><%= @suggestion.accepted_by_user_2 %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              suggestions_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_suggestion_path(@suggestion), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              suggestion_path(@suggestion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

